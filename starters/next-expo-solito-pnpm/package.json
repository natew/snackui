{
  "name": "myapp-monorepo",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "native": "cd apps/expo && pnpm start",
    "watch": "pnpm workspaces foreach -pi run watch",
    "vercel:install": "pnpm set version stable && pnpm install",
    "web": "pnpm build && cd apps/next && pnpm next",
    "web:extract": "DISABLE_EXTRACTION=false pnpm workspace next-app dev",
    "web:prod": "pnpm workspace next-app build",
    "web:prod:serve": "pnpm next-app serve",
    "postinstall": "pnpm build",
    "build": "pnpm turbo run build",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader && manypkg upgrade react-native-web-lite"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "tamagui": "^1.2.9"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "turbo": "^1.4.2",
    "typescript": "^4.7.4"
  },
  "packageManager": "pnpm@7.14.1"
}
