{
  "name": "tamagui-monorepo",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/tamagui/tamagui"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "takeout/*"
    ]
  },
  "scripts": {
    "postinstall": "manypkg check && yarn build:js",
    "check": "manypkg check",
    "build": "turbo run build",
    "build:force": "yarn clean:build && turbo run build --force",
    "build:js": "SKIP_TYPES=1 turbo run build",
    "build:types": "SKIP_JS=1 turbo run build",
    "watch": "npm-run-all --parallel watch:ts watch:build",
    "watch:ts": "yarn tsc -b -w --preserveWatchOutput tsconfig.build.json",
    "watch:build": "SKIP_TYPES_INITIAL=1 ultra -r --no-pretty --concurrency 100 watch",
    "clean:build": "yarn turbo run clean:build",
    "clean:ts": "yarn tsc -b --clean tsconfig.build.json",
    "clean": "yarn turbo run clean:build",
    "lint": "turbo run lint",
    "pretty": "pretty-quick",
    "pretty:all": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "turbo run test",
    "prettier": "prettier --write \"**/*.{ts,tsx}\" || true",
    "release:beta": "node ./release-next-beta.js",
    "publish": "lerna publish --preid beta",
    "publish:no-version": "npm publish --workspaces",
    "publish:canary": "lerna publish --canary --preid beta.0 --force-publish --dist-tag canary",
    "version": "lerna version",
    "kitchen-sink": "yarn workspace @tamagui/kitchen-sink start:clean",
    "kitchen-sink:web": "yarn workspace @tamagui/kitchen-sink web",
    "site": "cd packages/site && ./scripts/dev.sh",
    "site:extract": "yarn workspace @tamagui/site dev:extract",
    "site:prod": "yarn workspace @tamagui/site ci:build && yarn workspace @tamagui/site next:start",
    "site:prod:analyze": "ANALYZE=true yarn site:prod",
    "studio": "cd packages/kitchen-sink && ../../node_modules/.bin/tg dev",
    "sandbox:webpack": "yarn workspace @tamagui/sandbox dev:webpack",
    "sandbox": "yarn workspace @tamagui/sandbox dev",
    "sandbox:extract": "yarn workspace @tamagui/sandbox dev:extract",
    "fix": "yarn manypkg fix"
  },
  "resolutions": {
    "react-native-gesture-handler": "2.4.2",
    "react-native-reanimated": "3.0.0-rc.0",
    "@types/node": "16.11.9",
    "@types/react": "18.0.14",
    "@types/react-native": "0.66.3",
    "rollup-plugin-flat-dts": "^1.3.0",
    "esbuild": "0.14.42",
    "react-native": "0.68.2",
    "typescript": "4.7.4",
    "next": "12.2.1-canary.1",
    "webpack": "5.73.0",
    "prettier": "2.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "0.14.0",
    "react-native-web": "0.18.3",
    "takeout-monorepo": "portal:/Users/n8/takeout",
    "@takeout/font-noto": "portal:/Users/n8/takeout/packages/font-noto"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@babel/runtime": "^7.15.5",
    "@lerna-lite/cli": "^1.6.0",
    "@manypkg/cli": "^0.19.1",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.7.4",
    "@types/node": "^16.11.9",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "esbuild": "^0.14.48",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "import-sort-style-module": "^6.0.0",
    "node-gyp": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "pretty-quick": "^3.1.2",
    "prompts": "2.1.0",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4",
    "ultra-runner": "^3.10.5"
  }
}
