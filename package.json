{
  "name": "tamagui-monorepo",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/tamagui/tamagui"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "manypkg check && yarn build",
    "check": "manypkg check",
    "build": "yarn build:js && yarn build:types",
    "build:force": "yarn clean:build && yarn build:js --force && yarn build:types --force",
    "build:force:types": "yarn clean:build && yarn build:types --force",
    "build:js": "SKIP_TYPES=1 turbo run build",
    "build:types": "SKIP_JS=1 turbo run build",
    "watch": "npm-run-all --parallel watch:ts:preserve-output watch:build",
    "watch:ts": "yarn tsc -b -w tsconfig.build.json",
    "watch:ts:preserve-output": "yarn watch:ts --preserveWatchOutput",
    "watch:build": "SKIP_TYPES_INITIAL=1 ultra -r --no-pretty --concurrency 100 watch",
    "clean:build": "yarn turbo run clean:build",
    "clean": "yarn turbo run clean:build",
    "pretty": "pretty-quick",
    "pretty:all": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "turbo run test",
    "prettier": "prettier --write \"**/*.{ts,tsx}\" || true",
    "publish": "lerna publish",
    "publish:canary": "lerna publish --canary --preid beta.0 --force-publish --dist-tag canary",
    "version": "lerna version",
    "site": "yarn workspace @tamagui/site dev",
    "site:extract": "yarn workspace @tamagui/site dev:extract",
    "site:prod": "yarn workspace @tamagui/site next:build && yarn workspace @tamagui/site next:start",
    "sandbox": "yarn workspace @tamagui/sandbox dev",
    "sandbox:extract": "yarn workspace @tamagui/sandbox dev:extract",
    "fix": "yarn manypkg fix"
  },
  "resolutions": {
    "react-native-reanimated": "2.8.0",
    "@types/node": "16.11.9",
    "@types/react": "17.0.43",
    "@types/react-native": "0.66.3",
    "rollup-plugin-flat-dts": "^1.3.0",
    "esbuild": "0.14.38",
    "react-native": "0.0.0",
    "typescript": "4.6.3",
    "next": "12.1.5",
    "webpack": "5.72.0",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-refresh": "0.11.0",
    "react-native-web": "0.17.5",
    "tamagui": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/drawer": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/polyfill-dev": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/proxy-worm": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/use-controllable-state": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/use-debounce": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/compose-refs": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/core": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/helpers": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/build": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/static": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/core-node": "1.0.0-beta.0.209+5f91a5a5",
    "tamagui-loader": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/animate-presence": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/babel-plugin": "1.0.0-beta.0.209+5f91a5a5",
    "@tamagui/theme-base": "1.0.0-beta.0.209+5f91a5a5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@lerna-lite/cli": "^1.0.5",
    "@manypkg/cli": "^0.19.1",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.7.4",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "esbuild": "^0.14.36",
    "esbuild-jest": "^0.5.0",
    "import-sort-style-module": "^6.0.0",
    "node-gyp": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "pretty-quick": "^3.1.2",
    "turbo": "^1.1.10",
    "typescript": "^4.5.2",
    "ultra-runner": "^3.10.5"
  }
}
