{
  "name": "@tamagui/studio",
  "version": "1.61.1",
  "private": true,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./api": {
      "types": "./types/api/index.d.ts",
      "import": "./dist/esm/api/index.js",
      "require": "./dist/cjs/api/index.js"
    }
  },
  "scripts": {
    "dev": "../../node_modules/.bin/tama studio",
    "build": "tamagui-build",
    "ci:build": "tama studio --build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@supabase/auth-helpers-shared": "^0.4.1",
    "@tamagui/cli": "1.61.1",
    "@tamagui/linear-gradient": "1.61.1",
    "@tamagui/lucide-icons": "1.61.1",
    "@tamagui/theme": "1.61.1",
    "@tamagui/theme-builder": "1.61.1",
    "@tamagui/themes": "1.61.1",
    "@tamagui/use-store": "1.61.1",
    "color2k": "^2.0.2",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.6",
    "react-router-dom": "^6.14.1",
    "tamagui": "1.61.1",
    "tinybase": "^4.0.4",
    "victory": "^36.6.11"
  },
  "devDependencies": {
    "@tamagui/build": "1.61.1",
    "@types/react-color": "^3.0.6",
    "vite": "^4.4.9"
  }
}
