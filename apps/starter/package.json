{
  "name": "@tamagui/starter",
  "private": true,
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "test": "vitest type --run --config ../../packages/vite-plugin-internal/src/vite.config.ts **/*.test.tsx",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "postinstall": "yarn build",
    "build": "yarn workspaces foreach --exclude next-app -pi run build",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader && manypkg upgrade react-native-web-lite"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "@playwright/test": "^1.29.0",
    "esbuild-register": "^3.4.2",
    "playwright": "^1.29.0",
    "prettier": "^2.8.1",
    "turbo": "^1.8.3",
    "typescript": "^4.7.4",
    "vitest": "^0.26.3",
    "wait-port": "^0.3.0",
    "zx": "^7.1.1"
  },
  "packageManager": "yarn@3.2.3",
  "version": "1.11.2"
}
