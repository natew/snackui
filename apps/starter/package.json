{
  "name": "@tamagui/starter",
  "private": true,
  "scripts": {
    "test": "node -r esbuild-register ../../node_modules/.bin/playwright test integration.test.ts",
    "test:debug": "yarn test --debug",
    "native": "cd apps/expo && yarn start",
    "web": "cd apps/next && yarn next",
    "web:extract": "cd apps/next && yarn next",
    "web:prod": "yarn workspace next-app build",
    "web:prod:start": "yarn workspace next-app start",
    "watch": "yarn workspaces foreach -pi run watch",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader"
  },
  "resolutions": {
    "esbuild": "0.15.6",
    "@types/react": "17.0.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-web": "0.18.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "@manypkg/cli": "^0.19.1",
    "turbo": "^1.6.3",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.3"
}
