{
  "name": "@tamagui/bento",
  "version": "1.88.13",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --watch --skip-types",
    "clean": "tamagui-build clean",
    "dev:web": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "dev:web:prod": "NODE_ENV=production webpack serve",
    "dev:web:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "start": "EXPO_NO_TELEMETRY=true expo start --dev-client --offline",
    "start:extract": "TAMAGUI_OPTIMIZE_NATIVE_VIEWS=1 TAMAGUI_ENABLE_DYNAMIC_LOAD=1 yarn start",
    "start:prod": "DISABLE_EXTRACTION=false TAMAGUI_ENABLE_DYNAMIC_LOAD=1 expo start --dev-client --offline --no-dev --minify",
    "start:tamagui": "tama dev",
    "start:android": "yarn start --android",
    "start:clean": "watchman watch-del-all & rm -r $TMPDIR/metro-cache & yarn start -c",
    "pod": "npx pod-install",
    "android": "EXPO_NO_TELEMETRY=true yarn expo run:android",
    "ios": "yarn pod && EXPO_NO_TELEMETRY=true yarn expo run:ios",
    "web": "EXPO_NO_TELEMETRY=true yarn expo start --web",
    "prod:web": "NODE_ENV=production webpack --json=dist/compilation-stats.json"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native/polyfills": "^2.0.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@rehookify/datepicker": "^6.5.0",
    "@shopify/restyle": "^2.4.2",
    "@tamagui/animations-reanimated": "1.88.13",
    "@tamagui/cli": "1.88.13",
    "@tamagui/colors": "1.88.13",
    "@tamagui/config": "1.88.13",
    "@tamagui/core": "1.88.13",
    "@tamagui/demos": "1.88.13",
    "@tamagui/font-inter": "1.88.13",
    "@tamagui/lucide-icons": "1.88.13",
    "@tamagui/shorthands": "1.88.13",
    "@tamagui/themes": "1.88.13",
    "@tamagui/web": "1.88.13",
    "@tanstack/react-table": "^8.11.2",
    "burnt": "^0.12.1",
    "expo": "^49.0.21",
    "expo-build-properties": "~0.8.3",
    "expo-dev-client": "~2.4.12",
    "expo-document-picker": "^11.7.0",
    "expo-font": "~11.4.0",
    "expo-image-picker": "^14.5.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "^5.0.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-native": "^0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-ios-modal": "2.0.0-3",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^13.9.0",
    "react-native-web": "^0.19.10",
    "solito": "^4.0.0-alpha.4",
    "tamagui": "1.88.13"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@playwright/test": "^1.40.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@rnx-kit/metro-config": "^1.3.5",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.26",
    "@tamagui/babel-plugin": "1.88.13",
    "@tamagui/build": "1.88.13",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "html-webpack-plugin": "^5.5.0",
    "metro-runtime": "*",
    "mini-css-extract-plugin": "^2.7.6",
    "pod-install": "^0.1.38",
    "tilg": "0.1.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
