{
  "name": "@tamagui/site",
  "version": "1.5.23",
  "private": true,
  "source": "src",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "clean:build": "./scripts/clean.sh",
    "ci:build": "yarn next:build && yarn next:export",
    "ci:build:no-extract": "DISABLE_EXTRACTION=true yarn ci:build",
    "ci:build:debug": "DEBUG=tamagui yarn ci:build",
    "dev:analyze": "ANALYZE=true yarn dev",
    "dev:inspect": "PORT=${PORT:-5005} NODE_OPTIONS='--no-deprecation' NODE_ENV=development node --inspect-brk ../../node_modules/next/dist/bin/next dev",
    "dev:extract": "DISABLE_EXTRACTION=false yarn dev",
    "next:build": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next build",
    "next:export": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next export",
    "next:start": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next start --port 3333",
    "watch:site-data": "next-remote-watch ./data"
  },
  "dependencies": {
    "@docsearch/react": "^3.0.0",
    "@github/mini-throttle": "^2.1.0",
    "@stripe/stripe-js": "^1.29.0",
    "@supabase/supabase-auth-helpers": "^1.4.2",
    "@tamagui/animate-presence": "^1.5.23",
    "@tamagui/colors": "^1.5.23",
    "@tamagui/config": "^1.5.23",
    "@tamagui/demos": "^1.5.23",
    "@tamagui/font-fira-mono": "^1.5.23",
    "@tamagui/font-inter": "^1.5.23",
    "@tamagui/font-silkscreen": "^1.5.23",
    "@tamagui/logo": "^1.5.23",
    "@tamagui/lucide-icons": "^1.5.23",
    "@tamagui/menu": "^1.5.23",
    "@tamagui/next-theme": "^1.5.23",
    "@tamagui/remove-scroll": "^1.5.23",
    "@tamagui/shorthands": "^1.5.23",
    "@tamagui/themes": "^1.5.23",
    "compare-versions": "^4.1.3",
    "date-fns": "^2.26.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "hast-util-to-html": "^8.0.3",
    "hast-util-to-string": "^2.0.0",
    "mdx-bundler": "^9.0.1",
    "next": "^13.1.4",
    "nextjs-progressbar": "^0.0.14",
    "parse-numeric-range": "^1.3.0",
    "preact": "^10.11.0",
    "preact-render-to-string": "^5.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "*",
    "react-native-web-lite": "^1.5.23",
    "reading-time": "1.3.0",
    "refractor": "^4.7.0",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^5.0.1",
    "seamless-scroll-polyfill": "^1.2.3",
    "stripe": "^9.1.0",
    "tamagui": "^1.5.23",
    "unified": "^10.1.2",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.0",
    "@tamagui/next-plugin": "^1.5.23",
    "critters": "^0.0.16",
    "next-plugin-preact": "^3.0.7",
    "next-remote-watch": "^1.0.0",
    "tamagui-loader": "^1.5.23",
    "webpack-stats-plugin": "^1.1.0"
  }
}
