{
  "name": "@tamagui/kitchen-sink",
  "version": "1.120.2",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:one": "one dev",
    "start:web": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "start:web:prod": "NODE_ENV=production webpack serve",
    "start:web:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "start": "EXPO_NO_TELEMETRY=true expo start --dev-client --offline",
    "start:extract": "TAMAGUI_OPTIMIZE_NATIVE_VIEWS=1 TAMAGUI_ENABLE_DYNAMIC_LOAD=1 yarn start",
    "start:prod": "DISABLE_EXTRACTION=false TAMAGUI_ENABLE_DYNAMIC_LOAD=1 expo start --dev-client --offline --no-dev --minify",
    "start:tamagui": "tama dev",
    "start:android": "yarn start --android",
    "start:clean": "watchman watch-del-all & rm -r $TMPDIR/metro-cache & yarn start -c",
    "pod": "./pod-install.sh",
    "android": "EXPO_NO_TELEMETRY=true yarn expo run:android",
    "ios": "yarn pod && EXPO_NO_TELEMETRY=true yarn expo run:ios",
    "eject": "EXPO_NO_TELEMETRY=true expo eject",
    "screenshot": "node -r esbuild-register ../../node_modules/.bin/playwright test --config ./playwright-screenshot.config.ts",
    "test:native": "maestro test ./flows --exclude-tags=util",
    "test:web": "yarn test:web:driver-rn && yarn test:web:driver-css && yarn test:web:driver-moti",
    "test:web:driver-css": "NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=css node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:driver-rn": "NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=native node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:driver-moti": "NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=moti node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:debug": "yarn test:web --debug",
    "prod:web": "NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "test": "yarn test:web"
  },
  "dependencies": {
    "@dominicstop/ts-event-emitter": "^1.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.9.26",
    "@tamagui/animations-reanimated": "1.120.2",
    "@tamagui/cli": "1.120.2",
    "@tamagui/colors": "1.120.2",
    "@tamagui/config": "1.120.2",
    "@tamagui/core": "1.120.2",
    "@tamagui/demos": "1.120.2",
    "@tamagui/font-inter": "1.120.2",
    "@tamagui/lucide-icons": "1.120.2",
    "@tamagui/shorthands": "1.120.2",
    "@tamagui/themes": "1.120.2",
    "@tamagui/toast": "1.120.2",
    "@tamagui/web": "1.120.2",
    "@vxrn/vendor": "^1.1.335",
    "burnt": "^0.12.2",
    "expo": "^51.0.0",
    "expo-dev-client": "~4.0.13",
    "expo-document-picker": "~12.0.1",
    "expo-font": "~12.0.5",
    "expo-image-picker": "~15.0.4",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-splash-screen": "~0.27.4",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "2.20.0",
    "react-native-reanimated": "~3.11.0",
    "react-native-safe-area-context": "4.11.0",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.7.1",
    "react-native-web": "^0.19.0",
    "solito": "^4.2.2",
    "tamagui": "1.120.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.45.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tamagui/babel-plugin": "1.120.2",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.1",
    "one": "1.1.335",
    "pod-install": "^0.2.0",
    "tilg": "0.1.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
