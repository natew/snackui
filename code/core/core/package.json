{
  "name": "@tamagui/core",
  "version": "1.115.4",
  "source": "src/index.tsx",
  "main": "dist/cjs",
  "module": "dist/esm",
  "tamagui": {
    "bundle.native": "./src/index.tsx",
    "bundle.native.test": "./src/index.tsx"
  },
  "types": "./types/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Nate Wienert"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "files": [
    "types",
    "dist",
    "src",
    "reset.css",
    "native.js",
    "native.d.ts",
    "native-test.js",
    "native-test.d.ts"
  ],
  "dependencies": {
    "@tamagui/react-native-use-pressable": "1.115.4",
    "@tamagui/react-native-use-responder-events": "1.115.4",
    "@tamagui/use-event": "1.115.4",
    "@tamagui/web": "1.115.4"
  },
  "devDependencies": {
    "@tamagui/build": "1.115.4",
    "@testing-library/react": "^16.0.1",
    "csstype": "^3.0.10",
    "typescript": "^5.5.2",
    "vitest": "^2.1.3"
  },
  "exports": {
    "./package.json": "./package.json",
    "./reset.css": "./reset.css",
    ".": {
      "react-native-import": "./dist/esm/index.native.js",
      "react-native": "./dist/cjs/index.native.js",
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./native": {
      "types": "./types/index.d.ts",
      "import": "./dist/native.mjs",
      "require": "./dist/native.js",
      "react-native": "./dist/native.native.js"
    },
    "./native-test": {
      "types": "./types/index.d.ts",
      "import": "./dist/test.native.js",
      "require": "./dist/test.native.js",
      "react-native": "./dist/test.native.native.js"
    },
    "./inject-styles": {
      "types": "./types/inject-styles.d.ts",
      "import": "./dist/esm/inject-styles.mjs",
      "require": "./dist/cjs/inject-styles.cjs",
      "react-native": "./dist/cjs/inject-styles.native.js"
    },
    "./reactNativeTypes": {
      "types": "./types/reactNativeTypes.d.ts",
      "import": "./dist/esm/reactNativeTypes.mjs",
      "require": "./dist/cjs/reactNativeTypes.cjs",
      "react-native": "./dist/cjs/reactNativeTypes.native.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
