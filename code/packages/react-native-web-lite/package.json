{
  "name": "react-native-web-lite",
  "version": "1.111.2",
  "description": "React Native for Web",
  "sideEffects": false,
  "main": "dist/cjs",
  "module": "dist/cjs",
  "files": [
    "types",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --watch --skip-types",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native-import": "./dist/esm/index.native.js",
      "react-native": "./dist/cjs/index.native.js",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./without-animated": {
      "react-native-import": "./dist/esm/without-animated.native.js",
      "react-native": "./dist/cjs/without-animated.native.js",
      "import": "./dist/esm/without-animated.mjs",
      "require": "./dist/cjs/without-animated.js"
    },
    "./dist/modules/*": "./dist/esm/modules/*/index.js",
    "./dist/exports/*": "./dist/esm/exports/*/index.js",
    "./dist/*": "./dist/cjs/*/index.js"
  },
  "dependencies": {
    "@tamagui/normalize-css-color": "1.111.2",
    "@tamagui/react-native-use-pressable": "1.111.2",
    "@tamagui/react-native-use-responder-events": "1.111.2",
    "invariant": "^2.2.4",
    "react-native-web-internals": "1.111.2",
    "styleq": "^0.1.3"
  },
  "devDependencies": {
    "@tamagui/build": "1.111.2",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "license": "MIT",
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "packages/react-native-web-lite"
  },
  "bugs": {
    "url": "https://github.com/tamagui/tamagui/issues"
  }
}
