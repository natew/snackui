{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:js": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["tsconfig.json"],
      "outputs": [".next/**", "dist/**", "types/**"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build:js"],
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "clean:build": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": ["src/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
