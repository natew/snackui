{
  "name": "@tamagui/site-config",
  "version": "1.38.0",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@tamagui/animations-css": "1.38.0",
    "@tamagui/animations-react-native": "1.38.0",
    "@tamagui/colors": "1.38.0",
    "@tamagui/font-glusp": "1.38.0",
    "@tamagui/font-inter": "1.38.0",
    "@tamagui/font-silkscreen": "1.38.0",
    "@tamagui/react-native-media-driver": "1.38.0",
    "@tamagui/shorthands": "1.38.0",
    "@tamagui/themes": "1.38.0",
    "@tamagui/web": "1.38.0"
  },
  "devDependencies": {
    "@tamagui/build": "1.38.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
