{
  "name": "@tamagui/site-config",
  "version": "1.79.12",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "src",
    "types",
    "dist",
    "animations.css"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/biome check src",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./animations.css": {
      "types": "./types/animations.css.d.ts",
      "import": "./dist/esm/animations.js",
      "require": "./dist/cjs/animations.js"
    }
  },
  "dependencies": {
    "@tamagui/animations-react-native": "1.79.12",
    "@tamagui/animations-reanimated": "1.79.12",
    "@tamagui/colors": "1.79.12",
    "@tamagui/font-cherry-bomb": "1.79.12",
    "@tamagui/font-dm-sans": "1.79.12",
    "@tamagui/font-dm-serif-display": "1.79.12",
    "@tamagui/font-glusp": "1.79.12",
    "@tamagui/font-inter": "1.79.12",
    "@tamagui/font-silkscreen": "1.79.12",
    "@tamagui/react-native-media-driver": "1.79.12",
    "@tamagui/shorthands": "1.79.12",
    "@tamagui/themes": "1.79.12",
    "@tamagui/web": "1.79.12"
  },
  "devDependencies": {
    "@tamagui/build": "1.79.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
