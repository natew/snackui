{
  "name": "tamagui",
  "version": "1.1.12",
  "sideEffects": [
    "*.css",
    "*setup.js"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply-suggested src",
    "check": "yarn check-circular-deps",
    "check-circular-deps": "npx madge --circular ./src/index.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./linear-gradient": {
      "types": "./types/linear-gradient.d.ts",
      "import": "./dist/esm/linear-gradient.js",
      "require": "./dist/cjs/linear-gradient.js"
    }
  },
  "dependencies": {
    "@tamagui/adapt": "^1.1.12",
    "@tamagui/alert-dialog": "^1.1.12",
    "@tamagui/animate-presence": "^1.1.12",
    "@tamagui/avatar": "^1.1.12",
    "@tamagui/button": "^1.1.12",
    "@tamagui/card": "^1.1.12",
    "@tamagui/compose-refs": "^1.1.12",
    "@tamagui/core": "^1.1.12",
    "@tamagui/create-context": "^1.1.12",
    "@tamagui/dialog": "^1.1.12",
    "@tamagui/fake-react-native": "^1.1.12",
    "@tamagui/font-size": "^1.1.12",
    "@tamagui/get-button-sized": "^1.1.12",
    "@tamagui/get-font-sized": "^1.1.12",
    "@tamagui/get-size": "^1.1.12",
    "@tamagui/helpers": "^1.1.12",
    "@tamagui/helpers-tamagui": "^1.1.12",
    "@tamagui/image": "^1.1.12",
    "@tamagui/label": "^1.1.12",
    "@tamagui/linear-gradient": "^1.1.12",
    "@tamagui/list-item": "^1.1.12",
    "@tamagui/popover": "^1.1.12",
    "@tamagui/popper": "^1.1.12",
    "@tamagui/portal": "^1.1.12",
    "@tamagui/progress": "^1.1.12",
    "@tamagui/react-native-media-driver": "^1.1.12",
    "@tamagui/scroll-view": "^1.1.12",
    "@tamagui/select": "^1.1.12",
    "@tamagui/separator": "^1.1.12",
    "@tamagui/shapes": "^1.1.12",
    "@tamagui/sheet": "^1.1.12",
    "@tamagui/slider": "^1.1.12",
    "@tamagui/stacks": "^1.1.12",
    "@tamagui/switch": "^1.1.12",
    "@tamagui/text": "^1.1.12",
    "@tamagui/tooltip": "^1.1.12",
    "@tamagui/use-controllable-state": "^1.1.12",
    "@tamagui/use-debounce": "^1.1.12",
    "@tamagui/use-event": "^1.1.12",
    "@tamagui/use-force-update": "^1.1.12",
    "@tamagui/use-window-dimensions": "^1.1.12",
    "@tamagui/visually-hidden": "^1.1.12"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@tamagui/build": "^1.1.12",
    "@types/node": "^16.11.9",
    "@types/react": "^18.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.18.3"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
