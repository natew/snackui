    "@tamagui/dialog": "1.15.40",
    "@tamagui/fake-react-native": "1.15.40",
    "@tamagui/focusable": "1.15.40",
    "@tamagui/font-size": "1.15.40",
    "@tamagui/form": "1.15.40",
    "@tamagui/get-button-sized": "1.15.40",
    "@tamagui/get-font-sized": "1.15.40",
    "@tamagui/get-size": "1.15.40",
    "@tamagui/helpers": "1.15.40",
    "@tamagui/helpers-tamagui": "1.15.40",
