{
  "name": "@tamagui/cli",
  "version": "1.1.12",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "module": "dist/esm",
  "main": "dist/cjs",
  "bin": {
    "tama": "./dist/cjs/index.js",
    "tamagui": "./dist/cjs/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "types",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/build": "^1.1.12",
    "@tamagui/static": "^1.1.12",
    "@tamagui/vite-plugin": "^1.1.12",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.3",
    "arg": "^5.0.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "compression": "^1.7.4",
    "esbuild": "^0.17.5",
    "execa": "^5.0.0",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "get-port": "^6.1.2",
    "graphology": "^0.24.1",
    "kill-port": "^2.0.1",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "ts-morph": "^15.1.0",
    "typescript": "^4.7.4",
    "url": "^0.11.0",
    "vite": "^4.0.4",
    "zx": "^7.1.1"
  },
  "devDependencies": {
    "@takeout/studio": "^1.1.12"
  }
}
