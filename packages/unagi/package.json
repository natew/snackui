{
  "name": "@tamagui/unagi",
  "version": "1.0.1-beta.193",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "license": "MIT",
  "files": [
    "src",
    "types",
    "dist",
    "vendor"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    ".": "./dist/esm/index.js",
    "./experimental": {
      "import": "./dist/esm/experimental.js",
      "require": "./dist/cjs/experimental.js"
    },
    "./plugin": {
      "import": "./dist/esm/framework/plugin.js",
      "require": "./dist/cjs/framework/plugin.js"
    },
    "./plugin.cjs": "./plugin.cjs",
    "./middleware": {
      "import": "./dist/esm/framework/middleware.js",
      "require": "./dist/cjs/framework/middleware.js"
    },
    "./load-config": {
      "import": "./dist/esm/framework/loadConfig.js",
      "require": "./dist/cjs/framework/loadConfig.js"
    },
    "./web-polyfills": {
      "import": "./dist/esm/utilities/webPolyfill.js",
      "require": "./dist/cjs/utilities/webPolyfill.js"
    },
    "./vendor/*": "./vendor/*",
    "./cache/*": {
      "import": "./dist/esm/framework/cache/*.js",
      "require": "./dist/cjs/framework/cache/*.js"
    },
    "./platforms": {
      "types": "./dist/esm/platforms/virtual.d.ts",
      "import": "./dist/esm/platforms/virtual.js",
      "require": "./dist/cjs/platforms/virtual.js"
    },
    "./package.json": "./package.json",
    "./*": "./dist/esm/*.js"
  },
  "dependencies": {
    "@tamagui/proxy-worm": "^1.0.1-beta.193",
    "@tamagui/static": "^1.0.1-beta.193",
    "@tamagui/vite-plugin": "^1.0.1-beta.193",
    "@vitejs/plugin-react": "^1.3.0",
    "abort-controller": "^3.0.0",
    "connect": "^3.7.0",
    "es-module-lexer": "^0.9.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "history": "^5.3.0",
    "kolorist": "^1.5.1",
    "lodash": "^4.17.21",
    "magic-string": "^0.26.2",
    "node-fetch": "^2.6.7",
    "outdent": "^0.8.0",
    "path-to-regexp": "^6.2.1",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0",
    "set-cookie-parser": "^2.5.1",
    "undici": "^5.5.1",
    "vite-plugin-inspect": "^0.6.0",
    "web-streams-polyfill": "^3.2.1",
    "worktop": "^0.7.3"
  },
  "devDependencies": {
    "@shopify/react-testing": "^4.1.1",
    "@tamagui/build": "^1.0.1-beta.193",
    "@testing-library/jest-dom": "^5.16.4",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "react": "*",
    "react-dom": "*",
    "serve-static": "^1.14.1",
    "type-fest": "^2.18.0",
    "vite": "^3.0.8",
    "vite-plugin-environment": "^1.1.2",
    "vitest": "^0.20.3"
  },
  "peerDependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve-static": "^1.14.1",
    "vite": "^3.0.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
