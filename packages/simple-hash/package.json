{
  "name": "@tamagui/simple-hash",
  "version": "1.15.37",
  "sideEffects": false,
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/rome check src",
