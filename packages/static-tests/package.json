{
  "name": "@tamagui/static-tests",
  "version": "1.61.0",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "true",
    "pretest": "RUN_PRETEST=1 node -r esbuild-register ./tests/lib/preTest.ts",
    "test": "yarn test:native && yarn test:web && yarn test:webpack",
    "test:run": "INCLUDE_CSS_COLOR_NAMES=1 vitest --config ../vite-plugin-internal/src/vite.config.ts --run",
    "test:webpack": "yarn pretest && TAMAGUI_TARGET=web yarn test:run tests/webpack.test.tsx --reporter=verbose --singleThread",
    "test:flatten:native": "TAMAGUI_TARGET=native yarn test:run tests/flatten.native.test.tsx",
    "test:babel:native": "TAMAGUI_TARGET=native yarn test:run tests/babel.native.test.tsx",
    "test:babel:web": "TAMAGUI_TARGET=web yarn test:run tests/babel.web.test.tsx",
    "test:styled:web": "TAMAGUI_TARGET=web yarn test:run tests/styled.web.test.tsx",
    "test:web": "TAMAGUI_TARGET=web yarn test:run tests/*.web.test.tsx",
    "test:native": "TAMAGUI_TARGET=native yarn test:run tests/*.native.test.tsx"
  },
  "tests": {
    "parallel": true
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.21.4",
    "@babel/types": "^7.21.5",
    "@tamagui/sandbox-ui": "1.61.0",
    "@tamagui/test-design-system": "1.61.0",
    "@testing-library/react": "^14.0.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "@types/find-root": "^1.1.2",
    "@types/node": "^16.11.9",
    "@types/webpack": "^4.41.26",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.4",
    "dedent": "^0.7.0",
    "esbuild-loader": "^3.0.1",
    "null-loader": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.6",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "react-native-web": "*"
  }
}
