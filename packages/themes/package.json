{
  "name": "@tamagui/themes",
  "version": "1.40.0",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "generate": "tamagui generate-themes ./src/themes-new.ts ./src/generated-new.ts",
    "build": "tamagui-build",
    "watch": "yarn build --watch",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./src/themes-new.ts": "./src/themes-new.ts",
    "./generated.json": {
      "types": "./generated.d.ts",
      "import": "./generated.json",
      "require": "./generated.json"
    },
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@tamagui/colors": "1.40.0",
    "@tamagui/create-theme": "1.40.0",
    "@tamagui/theme-builder": "1.40.0",
    "@tamagui/web": "1.40.0"
  },
  "devDependencies": {
    "@tamagui/build": "1.40.0",
    "@tamagui/cli": "1.40.0",
    "vitest": "^0.26.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
