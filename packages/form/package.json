{
  "name": "@tamagui/form",
  "version": "1.15.30",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@tamagui/compose-refs": "1.15.30",
    "@tamagui/core": "1.15.30",
    "@tamagui/create-context": "1.15.30",
    "@tamagui/focusable": "1.15.30",
    "@tamagui/get-button-sized": "1.15.30",
    "@tamagui/get-font-sized": "1.15.30",
    "@tamagui/text": "1.15.30"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.15.30",
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
