    "@tamagui/core": "^1.0.1-beta.224",
    "@tamagui/list-item": "^1.0.1-beta.224",
    "@tamagui/logo": "^1.0.1-beta.224",
    "@tamagui/menu": "^1.0.1-beta.224",
    "@tamagui/popover": "^1.0.1-beta.224",
    "@tamagui/progress": "^1.0.1-beta.224",
    "@tamagui/select": "^1.0.1-beta.224",
    "@tamagui/sheet": "^1.0.1-beta.224",
    "@tamagui/slider": "^1.0.1-beta.224",
    "@tamagui/stacks": "^1.0.1-beta.224",
    "tamagui": "^1.0.1-beta.224"
