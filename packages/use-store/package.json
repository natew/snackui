  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@tamagui/simple-hash": "1.61.3"
  },
  "devDependencies": {
    "@tamagui/build": "1.61.3",
