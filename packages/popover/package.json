{
  "name": "@tamagui/popover",
  "version": "1.30.16",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@floating-ui/react": "~0.19.1",
    "@tamagui/adapt": "1.30.16",
    "@tamagui/animate-presence": "1.30.16",
    "@tamagui/aria-hidden": "1.30.16",
    "@tamagui/compose-refs": "1.30.16",
    "@tamagui/core": "1.30.16",
    "@tamagui/dismissable": "1.30.16",
    "@tamagui/floating": "1.30.16",
    "@tamagui/focus-scope": "1.30.16",
    "@tamagui/polyfill-dev": "1.30.16",
    "@tamagui/popper": "1.30.16",
    "@tamagui/portal": "1.30.16",
    "@tamagui/remove-scroll": "1.30.16",
    "@tamagui/scroll-view": "1.30.16",
    "@tamagui/sheet": "1.30.16",
    "@tamagui/stacks": "1.30.16",
    "@tamagui/use-controllable-state": "1.30.16"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.30.16",
    "react": "^18.2.0",
    "react-native": "^0.71.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
